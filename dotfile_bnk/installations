	Compass
		sudo gem install compass
	JPEGTran
		brew install jpeg-turbo && brew link jpeg-turbo
	OptiPNG
		brew install optipng
	PhantomJS
		brew install phantomjs
	Yeoman
		sudo npm install -g yeoman
